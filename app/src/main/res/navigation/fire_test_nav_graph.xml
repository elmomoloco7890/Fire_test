<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fire_test_nav_graph"
    app:startDestination="@id/fireHomeFragment">

    <fragment
        android:id="@+id/fireHomeFragment"
        android:name="com.artemis.fire_test.fragments.FireHomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_fire_home">
        <action
            android:id="@+id/action_fireHomeFragment_to_fireNestedFragment"
            app:destination="@id/fireNestedFragment" />
    </fragment>
    <fragment
        android:id="@+id/fireNestedFragment"
        android:name="com.artemis.fire_test.fragments.FireNestedFragment"
        android:label="Nested"
        tools:layout="@layout/fragment_fire_nested">
        <action
            android:id="@+id/action_fireNestedFragment_to_fireDetailFragment"
            app:destination="@id/fireDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/fireDetailFragment"
        android:name="com.artemis.fire_test.fragments.FireDetailFragment"
        android:label="fragment_fire_detail"
        tools:layout="@layout/fragment_fire_detail" >
        <action
            android:id="@+id/action_fireDetailFragment_to_fireNestedFragment"
            app:destination="@id/fireNestedFragment" />
        <action
            android:id="@+id/action_fireDetailFragment_to_fireHomeFragment"
            app:destination="@id/fireHomeFragment" />
    </fragment>
</navigation>